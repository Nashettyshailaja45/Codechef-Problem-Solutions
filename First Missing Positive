import heapq
class Solution:
    def firstMissingPositive(self, nums: List[int]) -> int:
        heap=[]
        for n in nums:
            if(n>0):
                heapq.heappush(heap,n)
        mp=1
        while heap:
            s=heapq.heappop(heap)
            if(s==mp):
                mp+=1

